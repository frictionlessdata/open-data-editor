  "description": "Data management for humans",
  "author": "Frictionless Data",
  "homepage": "https://frictionlessdata.io",
  "main": "lib/application.js",
  "engines": {
    "node": "^16.0.0"
  },
  "files": [
    "dist",
    "lib",
    "src",
    "test"
