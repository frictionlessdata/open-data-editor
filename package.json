    "url": "https://github.com/frictionlessdata/application/issues"
  },
  "scripts": {
    "analyze": "NODE_ENV=production webpack --profile --json | webpack-bundle-size-analyzer",
    "application": "concurrently npm:server npm:client",
    "build": "npm run compile && npm run bundle",
    "bundle": "NODE_ENV=production webpack",
    "client": "webpack serve --open",
    "compile": "rimraf lib && tsc",
    "components": "start-storybook -p 6006",
    "coverage": "sensible-browser coverage/index.html",
    "debug": "ndb node -r ts-node/register/transpile-only",
    "format": "prettier --write '{src,test}/**/*.ts*' && eslint --fix '{src,test}/**/*.ts*'",
    "lint": "prettier --check '{src,test}/**/*.ts*' && eslint '{src,test}/**/*.ts*'",
    "metadata": "NODE_ENV=production ENTRY=metadata webpack",
    "pretest": "npm run lint",
    "server": "frictionless api",
    "start": "node lib/application.js",
    "test": "jest --coverage",
    "update": "ncu -u"
  },
